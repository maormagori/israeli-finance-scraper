{
    "dependencies": {
        "@types/lodash": "^4.14.123",
        "chrome-aws-lambda": "^1.17.1",
        "csv-parse": "^4.4.1",
        "firebase-admin": "^7.3.0",
        "firebase-functions": "^2.3.1",
        "israeli-bank-scrapers": "orzarchi/israeli-bank-scrapers",
        "locate-chrome": "^0.1.1",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "shortid": "^2.2.14",
        "ynab": "^1.12.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.26",
        "@types/node": "^11.13.2",
        "@types/puppeteer": "^1.12.3",
        "@types/puppeteer-core": "^1.9.0",
        "@types/shortid": "^0.0.29",
        "nodemon": "^1.18.11",
        "prettier": "^1.16.4",
        "ts-node": "^8.0.3",
        "tsconfig-paths": "^3.8.0",
        "tslint": "^5.15.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.4.2"
    },
    "scripts": {
        "build": "tsc",
        "watch-ts": "tsc -w",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "scrape": "ts-node src/commands/scrape.ts",
        "ynab": "ts-node src/commands/uploadToYnab.ts",
        "start:dev": "nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "package": "rm dist.zip; npm run build && cd lib && zip -r ../dist.zip * && cd .. && zip -r dist.zip ./node_modules",
        "deploy": "aws lambda update-function-code --function-name finance-scraper --zip-file fileb://./dist.zip",
        "postinstall": "cd node_modules/israeli-bank-scrapers && npm install --no-optional && npm run prepublishOnly && rm -rf node_modules"
    }
}
